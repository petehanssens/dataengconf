---
import Venue from '../../components/conference/venue.astro'
import Speaker from '../../components/conference/speakers.astro'
import Sessions from '../../components/conference/session.astro'
import Layout from '../../layouts/layout.astro';
const apiUrl="https://sessionize.com/api/v2/xvrqdvp3/view/All";
const details = {
  city: "Sydney",
  image: "/images/sydney.webp",
  day: "Friday",
  date: "25th August",
  venue: "Hilton Sydney",
  address: "488 George St, Sydney NSW 2000",
  url: "https://www.tickettailor.com/events/dataengbytes/859971" 
}
---
<Layout  title="Sydney Data Engineering Conference" description="Sydney Conference Details">
    <div>
        <Venue {details}/>
        <Speaker apiUrl={apiUrl}/>
        <Sessions apiUrl={apiUrl}/>
        <img src="/banners/speakers.webp" alt="Our international speakers" class="w-full flex " />
        <img src="/banners/org-sydney.webp" alt="Our volunteers" class="w-full flex " />
        <img src="/banners/org-core-team.webp" alt="Our core team of oganisers" class="w-full flex " />        
    </div>
</Layout>