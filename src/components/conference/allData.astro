export async function fetchData(apiUrl) {
//   const apiUrl = "https://sessionize.com/api/v2/2gv8oeqy/view/All";

  try {
    const response = await fetch(apiUrl);

    const data = await response.json();
    
    return data;
  } catch (error) {
    console.error("Error fetching data:", error);
    throw error;
  }
}

